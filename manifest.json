{
  "manifest_version": 2,
  "name": "Phishing Detection",
  "version": "0.1",
  "icons":{ "16":"c.png",
            "48": "c.png",
            "128": "c.png"},
  "description": "when clicked checks if the URL is a phishing website or not",
  "web_accessible_resources":[
    "c.png"
  ],
  "content_scripts": [
    {
      "matches":[
        "<all_urls>"
      ],
      "js":["content.js"]
    }
  ],
  "permissions": [
    "tabs",
    "storage"
  ],
  "background":{
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_icon":"c2.png",
    "default_popup":"popup.html",
    "default_title":"Phishing?"
  }
}
